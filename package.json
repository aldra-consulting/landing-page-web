{
  "name": "landing-page-web",
  "description": "Organisation's landing page.",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:aldra-consulting/landing-page-web.git"
  },
  "contributors": [
    {
      "name": "Alexander Zakharov (azaxarov)",
      "email": "alexander.zakharov@aldra.no"
    }
  ],
  "license": "AGPL-3.0",
  "private": true,
  "files": [
    "dist/**"
  ],
  "engines": {
    "node": ">= 18.12.1"
  },
  "scripts": {
    "start": "run-s serve:dev",
    "debug": "run-s serve:debug",
    "serve:dev": "vite --mode ssr",
    "serve:debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "build": "run-s clean:dist typescript:compile build:qwik",
    "build:qwik": "qwik build",
    "build.client": "vite build",
    "build.server": "vite build -c adaptors/static/vite.config.ts",
    "build:preview": "vite build --ssr src/entry.preview.tsx",
    "typescript:compile": "tsc",
    "lint:project": "run-p lint:eslint:project lint:stylelint:project",
    "lint:project:fix": "run-p lint:eslint:project:fix lint:stylelint:project:fix",
    "lint:eslint:project": "eslint --ext ts,js ./",
    "lint:eslint:project:fix": "eslint --ext ts,js ./ --fix",
    "lint:eslint:staged:fix": "eslint --ext ts,js --fix",
    "lint:stylelint:project": "stylelint ./src/**/*.css",
    "lint:stylelint:project:fix": "stylelint ./src/**/*.css --fix",
    "lint:stylelint:staged:fix": "stylelint --fix",
    "clean:dist": "rm -rf dist",
    "clean:coverage": "rm -rf coverage",
    "test": "run-s clean:coverage test:jest",
    "test:coverage": "run-s clean:coverage test:jest:coverage",
    "test:jest": "jest --passWithNoTests",
    "test:jest:coverage": "jest --passWithNoTests --coverage",
    "test:watch": "jest --passWithNoTests --watchAll",
    "commit": "git-cz",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@builder.io/qwik": "^0.14.1",
    "@builder.io/qwik-city": "^0.0.127",
    "@builder.io/qwik-react": "0.1.1",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.2.3",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qwik": "^0.14.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "semantic-release": "^19.0.5",
    "stylelint": "^14.15.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-tsconfig-paths": "^3.6.0"
  }
}
